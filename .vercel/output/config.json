{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/analytics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/health\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/fullcircle\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/global\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/maintenance\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/offshore\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/statistics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/technicians\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/turbines\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/weather\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}